{
  "name": "cubera-proto",
  "version": "0.5.0",
  "private": true,
  "dependencies": {
    "@azure/storage-blob": "^12.17.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.9",
    "@uniswap/lib": "^4.0.1-alpha",
    "@uniswap/v2-core": "^1.0.1",
    "bignumber.js": "^9.0.1",
    "blockchain-addressbook": "^0.44.19",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "keccak": "^3.0.1",
    "node-fetch": "2.6.1",
    "npm-run-all": "^4.1.5",
    "rlp": "^2.2.6",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "configs:copy:vaults": "copyfiles -f ./configs/vault/*.json ./configs/vault/local/",
    "configs:copy:zaps": "copyfiles -f ./configs/zap/*.json ./configs/zap/local/",
    "configs:copy:amms": "copyfiles -f ./configs/amm/*.json ./configs/amm/local/",
    "configs:copy:boost": "copyfiles -f ./configs/boost/*.json ./configs/boost/local/",
    "configs:copy:cubera": "copyfiles -f ./configs/cubera/*.json ./configs/cubera/local/",
    "configs:copy": "run-s configs:copy:*",
    "clean": "hardhat clean --config ./hardhat.config.build.ts",
    "compile": "hardhat compile --config ./hardhat.config.build.ts",
    "typechain": "hardhat typechain --config ./hardhat.config.build.ts",
    "build": "yarn clean && yarn compile",
    "hhf:eth": "cross-env FORK_NETWORK=ethereum hardhat --network localhost",
    "hhf:op": "cross-env FORK_NETWORK=optimism hardhat --network localhost",
    "hhf:bsc": "cross-env FORK_NETWORK=bsc hardhat --network localhost",
    "hhf:poly": "cross-env FORK_NETWORK=polygon hardhat --network localhost",
    "hhf:avax": "cross-env FORK_NETWORK=avax hardhat --network localhost",
    "hhf:arb": "cross-env FORK_NETWORK=arbitrum hardhat --network localhost",
    "hhf:base": "cross-env FORK_NETWORK=base hardhat --network localhost",
    "hhf:eth:deploy": "yarn hhf:eth run scripts/deployment/deploy/ethereum/deploy-all.ts",
    "hhf:bsc:deploy": "yarn hhf:bsc run scripts/deployment/deploy/bsc/deploy-all.ts",
    "hhf:poly:deploy": "yarn hhf:poly run scripts/deployment/deploy/polygon/deploy-all.ts",
    "hhf:arb:deploy": "yarn hhf:arb run scripts/deployment/deploy/arbitrum/deploy-all.ts",
    "hhf:op:deploy": "yarn hhf:op run scripts/deployment/deploy/optimism/deploy-all.ts",
    "hhf:avax:deploy": "yarn hhf:avax run scripts/deployment/deploy/avax/deploy-all.ts",
    "hhf:base:deploy": "yarn hhf:base run scripts/deployment/deploy/base/deploy-all.ts",
    "avax:deploy": "yarn hardhat --network avax run scripts/deployment/deploy/avax/deploy-all.ts",
    "base:deploy": "yarn hardhat --network base run scripts/deployment/deploy/base/deploy-all.ts",
    "op:deploy": "yarn hardhat --network optimism run scripts/deployment/deploy/optimism/deploy-all.ts",
    "arb:deploy": "yarn hardhat --network arbitrum run scripts/deployment/deploy/arbitrum/deploy-all.ts",
    "bsc:deploy": "yarn hardhat --network bsc run scripts/deployment/deploy/bsc/deploy-all.ts",
    "poly:deploy": "yarn hardhat --network polygon run scripts/deployment/deploy/polygon/deploy-all.ts",
    "node:eth": "cross-env FORK_NETWORK=ethereum hardhat node --port 8545",
    "node:bsc": "cross-env FORK_NETWORK=bsc hardhat node --port 9545",
    "node:poly": "cross-env FORK_NETWORK=polygon hardhat node --port 10545",
    "node:arb": "cross-env FORK_NETWORK=arbitrum hardhat node --port 11545",
    "node:op": "cross-env FORK_NETWORK=optimism hardhat node --port 12545",
    "node:avax": "cross-env FORK_NETWORK=avax hardhat node --port 13545",
    "node:base": "cross-env FORK_NETWORK=base hardhat node --port 14545",
    "test": "yarn compile && cross-env TEST=TRUE hardhat test",
    "test:arb": "cross-env FORK_NETWORK=arbitrum yarn test",
    "test:poly": "cross-env FORK_NETWORK=polygon yarn test",
    "test:eth": "cross-env FORK_NETWORK=ethereum yarn test",
    "test:op": "cross-env FORK_NETWORK=optimism yarn test",
    "test:avax": "cross-env FORK_NETWORK=avax yarn test",
    "test:base": "cross-env FORK_NETWORK=base yarn test",
    "test:vault": "hardhat test --network localhost test/prod/VaultLifecycle.test.js",
    "test:upgrade": "hardhat test --network localhost test/prod/StratTimelockUpgrade.test.js",
    "test:registry": "hardhat test --network localhost test/infra/VaultRegistry.test.ts",
    "test:harvester": "hardhat test --network localhost test/infra/AutoHarvester.test.ts",
    "zap": "hardhat run ./scripts/infra/zapViaAggregator.js --network",
    "flat": "bin/flatten.sh",
    "flat:chain": "bin/flattenChainContracts.sh",
    "abi-to-sol": "abi-to-sol",
    "installForge": ". scripts/forge/installForge.sh",
    "forgeTest:vault": "forge test --force --fork-url https://rpc.ftm.tools --match-contract ProdVaultTest",
    "forgeTest:wrapper": "forge test --force --fork-url https://arb1.arbitrum.io/rpc --match-contract WrapperTest",
    "forgeTest:aura": "forge test --force --fork-url https://rpc.ankr.com/eth --match-contract StrategyAuraGyroTest"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.8.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin-2/contracts": "npm:@openzeppelin/contracts@2.5.1",
    "@openzeppelin-4/contracts": "npm:@openzeppelin/contracts@4.7.2",
    "@openzeppelin/contracts-upgradeable": "^4.9.3",
    "@openzeppelin/hardhat-upgrades": "1.28.0",
    "@openzeppelin/upgrades-core": "^1.20.4",
    "@typechain/ethers-v5": "^11.1.2",
    "@typechain/hardhat": "^3.0.0",
    "@types/chai": "^4.3.0",
    "@types/keccak": "^3.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-periphery": "^1.4.1",
    "abi-to-sol": "^0.5.2",
    "axios": "^0.21.2",
    "chai": "^4.2.0",
    "dotenv": "^10.0.0",
    "ds-test": "https://github.com/dapphub/ds-test.git#9310e879db8ba3ea6d5c6489a579118fd264a3f5",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eth-multicall": "^1.4.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.2",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#fc560fa34fa12a335a50c35d92e55a6628ca467c",
    "hardhat": "^2.18.3",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.9",
    "mocha": "^9.1.1",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
    "truffle-flattener": "^1.6.0",
    "typechain": "^8.3.2",
    "web3": "^1.6.0"
  }
}
